{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.autocomplete": "default",
    "C_Cpp.formatting": "vcFormat",
    "C_Cpp.vcFormat.indent.braces": false,
    "C_Cpp.vcFormat.indent.multiLineRelativeTo": "innermost",
    "C_Cpp.vcFormat.indent.preserveIndentationInComments": true,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
    "C_Cpp.vcFormat.newLine.withinEmptyBlock": "remove",
    "C_Cpp.vcFormat.space.aroundPointerOperators": "remove",
    "C_Cpp.vcFormat.space.beforeFunctionOpenParenthesis": "remove",
    "C_Cpp.vcFormat.space.withinCastParentheses": "remove",
    "C_Cpp.vcFormat.space.withinExpressionParentheses": "remove",
    "C_Cpp.vcFormat.space.withinFunctionCallParentheses": "remove",
    "C_Cpp.vcFormat.space.withinFunctionDeclarationParentheses": "remove",
    "C_Cpp.vcFormat.space.withinIfWhileForSwitchParentheses": "remove",
    "C_Cpp.vcFormat.space.withinMethodCallParentheses": "remove",
    "C_Cpp.vcFormat.space.withinMethodDeclarationParentheses": "remove",
    "C_Cpp.vcFormat.space.withinParentheses": "remove",
    "C_Cpp.vcFormat.space.withinTemplateParameters": "remove",
    "C_Cpp.vcFormat.space.withinTypeReferenceParentheses": "remove",
    "C_Cpp.vcFormat.space.withinVectorInitializerBraces": "remove",
    "C_Cpp.vcFormat.space.withinCastParentheses": "remove",
    "C_Cpp.vcFormat.space.withinExpressionParentheses": "remove",
    "C_Cpp.vcFormat.space.withinFunctionCallParentheses": "remove",
    "C_Cpp.vcFormat.space.withinFunctionDeclarationParentheses": "remove",
    "C_Cpp.vcFormat.space.withinIfWhileForSwitchParentheses": "remove",
    "C_Cpp.vcFormat.space.withinMethodCallParentheses": "remove",
    "C_Cpp.vcFormat.space.withinMethodDeclarationParentheses": "remove",
    "C_Cpp.vcFormat.space.withinParentheses": "remove",
    "C_Cpp.vcFormat.space.withinTemplateParameters": "remove",
    "C_Cpp.vcFormat.space.withinTypeReferenceParentheses": "remove",
    "C_Cpp.vcFormat.space.withinVectorInitializerBraces": "remove",
    "files.associations": {
        "*.hpp": "cpp",
        "*.h": "cpp",
        "*.cpp": "cpp",
        "*.cc": "cpp",
        "*.cxx": "cpp",
        "*.c": "c"
    },
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.rulers": [
        80,
        120
    ],
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true
}
